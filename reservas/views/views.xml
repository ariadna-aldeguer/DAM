    <!-- Top menu item -->

    <menuitem name="reservas_ariadna" id="reservas_ariadna.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Habitacion" id="reservas_ariadna.menu_room" parent="reservas_ariadna.menu_root" action="reservas_ariadna.room_action_window"/>
    <menuitem name="Clientes" id="reservas_ariadna.menu_client" parent="reservas_ariadna.menu_root" action="reservas_ariadna.client_action_window"/>

