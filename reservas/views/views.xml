<odoo>
  <data>
    <!-- VISTA DE HABITACION TREE-->

    <record model="ir.ui.view" id="reservas_ariadna.room_list_view">
      <field name="name">reservas_ariadna.room.view.tree</field>
      <field name="model">reservas_ariadna.room</field>
      <field name="arch" type="xml">
        <tree>
          <field name="num_hab"/>
          <field name="pis"/>
          <field name="tv"/>
          <field name="max_pax"/>
        </tree>
      </field>
    </record>

    <!-- VISTA DE CLIENTE TREE-->
    <record model="ir.ui.view" id="reservas_ariadna.client_list_view">
      <field name="name">reservas_ariadna.client.view.tree</field>
      <field name="model">reservas_ariadna.client</field>
      <field name="arch" type="xml">
        <tree>
          <field name="nom"/>
          <field name="DNI"/>
          <field name="edat"/>
          <field name="naix"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->
   <record model="ir.actions.act_window" id="reservas_ariadna.room_action_window">
      <field name="name">Habitacion</field>
      <field name="res_model">reservas_ariadna.room</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="reservas_ariadna.client_action_window">
      <field name="name">Cliente</field>
      <field name="res_model">reservas_ariadna.client</field>
      <field name="view_mode">tree,form</field>
    </record>


